{
  "name": "@re-shell/cli",
  "version": "0.2.0",
  "description": "CLI tool for Re-Shell microfrontend framework",
  "main": "dist/index.js",
  "bin": {
    "re-shell": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "microfrontend",
    "react",
    "re-shell",
    "cli",
    "framework",
    "micro-frontend"
  ],
  "author": "Re-Shell Organization",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Re-Shell/reshell-monorepo.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "fs-extra": "^11.0.0",
    "prompts": "^2.0.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vitest": "^0.34.3"
  }
}